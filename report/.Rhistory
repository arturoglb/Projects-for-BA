Mean=mean(val, na.rm=TRUE),
Min=min(val, na.rm=TRUE),
Max=max(val, na.rm=TRUE),
Percent_25=quantile(val, probs=c(.25), na.rm=TRUE),
Percent_75=quantile(val, probs=c(.75), na.rm=TRUE))
